---
source: crates/agglayer/tests/config.rs
expression: sanitize_config_folder_path(result)
---
[full-node-rpcs]

[l2]
rpc-timeout = 45

[proof-signers]

[log]
level = "info"
outputs = []
format = "pretty"

[rpc]
port = 9090
host = "0.0.0.0"
request-timeout = 180

[outbound.rpc.settle]
max-retries = 3
retry-interval = 7
confirmations = 1

[l1]
chain-id = 1337
node-url = "http://zkevm-mock-l1-network:8545/"
rollup-manager-contract = "0xb7f8bc63bbcad18155201308c8f3540b07f84f5e"
rpc-timeout = 45

[auth.local]
private-keys = []

[telemetry]
prometheus-addr = "0.0.0.0:3000"

[epoch.time-clock]
epoch-duration = 5

[shutdown]
runtime-timeout = 5

[certificate-orchestrator]
input-backpressure-buffer-size = 1000

[certificate-orchestrator.prover.sp1-local]

[storage]
metadata-path = "/tmp/agglayer-test/storage/metadata"
pending-path = "/tmp/agglayer-test/storage/pending"
state-path = "/tmp/agglayer-test/storage/state"
epochs-path = "/tmp/agglayer-test/storage/epochs"
