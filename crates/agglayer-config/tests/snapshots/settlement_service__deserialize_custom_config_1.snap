[retry-on-transient-failure]
initial-interval = "15s"
interval-multiplier-factor = 1.5
max-interval = "2m"
jitter = "1s"

[retry-on-not-included-on-l1]
initial-interval = "1m"
interval-multiplier-factor = 2.0
max-interval = "10m"
jitter = "10s"
